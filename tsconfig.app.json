{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"],
    "baseUrl": "src",
    "paths": {
      "@env/*": [
        "./environments/*"
      ],
      "@providers/*": [
        "./app/providers/*"
      ],
    },
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
